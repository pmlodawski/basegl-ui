{
  "name": "luna-basegl-ui",
  "license": "AGPL-3.0",
  "version": "0.0.8",
  "description": "Luna node editor using basegl.",
  "homepage": "https://github.com/luna/basegl-ui",
  "author": "The Luna Team <contact@luna-lang.org>",
  "main": "./dist/js/bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "watch": "webpack --progress --watch",
    "start": "webpack-dev-server --open",
    "prepublish": "npm run build",
    "postpublish": "rm dist/js/bundle.js",
    "deploy": "npm install && git commit -am \"Version bump\" && git push origin basegl-npm && npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luna/basegl-ui.git"
  },
  "bugs": {
    "url": "https://github.com/luna/basegl-ui/issues"
  },
  "devDependencies": {
    "@luna-lang/jsnext": "^0.0.10",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "next",
    "gl-matrix": "^2.4.0",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "basegl": "0.2.26",
    "bluebird": "^3.5.1",
    "glslify-loader": "^1.0.2",
    "raw-loader": "^0.5.1",
    "underscore": "^1.9.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.5.0"
          },
          "shippedProposals": true
        }
      ]
    ]
  }
}
